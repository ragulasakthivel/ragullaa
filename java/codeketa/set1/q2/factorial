import java.util.*;
import java.lang.*;
import java.io.*;
class factorial
{  
    public static void main(String args[])
    {
     Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int fact=1;
     for(int i=1;i<=5;i++)
      fact=fact*i;
      System.out.println( " factorial= "  + fact);
    }
 
}
